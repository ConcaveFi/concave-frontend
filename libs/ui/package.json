{
  "name": "@concave/ui",
  "version": "0.0.0",
  "description": "Secret concave ui library",
  "license": "MIT",
  "scripts": {
    "gen:theme-typings": "chakra-cli tokens ./src/theme/index.ts",
    "postinstall": "yarn gen:theme-typings",
    "build": "tsup --inject inject-react.js",
    "dev": "tsup --watch --inject inject-react.js",
    "prepack": "yarn gen:theme-typings && yarn build",
    "publish": "yarn npm publish"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "tsup": {
    "entry": [
      "src/index.tsx",
      "src/components/*"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "treeshake": true,
    "sourcemap": true,
    "clean": true,
    "silent": true
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "tsup": "^6.1.3"
  },
  "dependencies": {
    "@chakra-ui/cli": "^2.1.0",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "framer-motion": "^6.3.16",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^4.9.3"
  }
}
