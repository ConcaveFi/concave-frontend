
{
  "name": "@concave/icons",
  "version": "0.0.0",
  "description": "Concave icon library",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup --inject inject-react.js",
    "dev": "tsup --watch --inject inject-react.js"
  },
  "files": [
    "dist"
  ],
  "tsup": {
    "entry": [
      "src/**"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "splitting": true,
    "treeshake": true,
    "sourcemap": true,
    "clean": true,
    "silent": true
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "react": "^18.2.0",
    "react-icons": "^4.4.0"
  }
}
