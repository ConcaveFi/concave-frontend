{
  "name": "@concave/core",
  "version": "0.0.0",
  "description": "Concave Core library",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConcaveFi/concave-frontend.git",
    "directory": "libs/core"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "tsdx test --passWithNoTests"
  },
  "tsup": {
    "entry": [
      "src/**"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "splitting": true,
    "treeshake": true,
    "sourcemap": true,
    "clean": true,
    "silent": true
  },
  "dependencies": {
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/solidity": "^5.6.1",
    "@ethersproject/wallet": "^5.6.2",
    "@types/big.js": "^6.1.5",
    "big.js": "^6.2.1",
    "decimal.js-light": "^2.5.1",
    "jsbi": "^4.3.0",
    "prettier": "^2.7.1",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3",
    "toformat": "^2.0.0"
  },
  "devDependencies": {
    "tsdx": "^0.14.1"
  }
}
