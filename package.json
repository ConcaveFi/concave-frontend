{
  "private": true,
  "scripts": {
    "build": "turbo build --filter core",
    "clean": "turbo clean",
    "dev:cave": "turbo run dev --filter=cave...",
    "dev": "turbo dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test --filter=!cave"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "^1.9.3"
  },
  "packageManager": "pnpm@7.15.0"
}
