{
  "name": "concave-frontend",
  "author": "concave co-op",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "gen:theme-typings": "chakra-cli tokens ./theme/index.ts",
    "postinstall": "npm run gen:theme-typings",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "gen": "DOTENV_CONFIG_PATH=./.env graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.7.4",
    "@chakra-ui/theme-tools": "^1.3.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/pt-sans": "^4.5.0",
    "@usedapp/core": "^0.9.0",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "ethers": "^5.5.3",
    "framer-motion": "^6.0.0",
    "graphql": "^16.2.0",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-number-format": "^4.9.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.2.1",
    "prettier": "^2.5.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.5.4"
  }
}
