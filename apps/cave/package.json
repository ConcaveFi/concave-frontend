{
  "name": "cave",
  "author": "concave co-op",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn resolve-assets-symlinks && next build",
    "local-build": "next build",
    "start": "next start",
    "symlink-assets": "globstar --node -- lnk \"../../assets/*\" ./public/assets",
    "resolve-assets-symlinks": "find ./public/assets -lname '*' -delete && cp -a ../../assets/. ./public/assets",
    "lint": "next lint --fix",
    "test": "jest --watch",
    "gen": "DOTENV_CONFIG_PATH=./.env graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@0xsequence/multicall": "^0.36.8",
    "@apollo/client": "^3.5.10",
    "@concave/icons": "*",
    "@concave/ui": "*",
    "@graphql-codegen/typescript-react-query": "^3.5.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-sdk": "^3.0.1",
    "aes256": "^1.1.0",
    "cookie": "^0.4.2",
    "ethers": "^5.6.0",
    "globstar": "^1.0.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "jest": "^27.5.1",
    "lightweight-charts": "^3.8.0",
    "lnk-cli": "^1.0.1",
    "next": "^12.1.0",
    "nock": "^13.2.4",
    "node-mocks-http": "^1.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-number-format": "^4.9.1",
    "react-query": "^3.34.16",
    "react-use": "^17.3.2",
    "siwe": "^1.1.2",
    "wagmi": "^0.2.18"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-react-query": "^3.5.7",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40"
  }
}
