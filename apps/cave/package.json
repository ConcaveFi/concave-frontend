{
  "name": "cave",
  "author": "concave co-op",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn fix-assets-symlink && next build",
    "win": "yarn fix-assets-symlink-win",
    "fix-assets-symlink": "rimraf ./public/assets && syncdir ../../assets ./public/assets --copy",
    "fix-assets-symlink-win": "xcopy /D /I /E /F /Y /H /R \"../..//assets\" \"./public/assets\\\"",
    "start": "next start",
    "lint": "next lint --fix",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "gen": "DOTENV_CONFIG_PATH=./.env graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@concave/icons": "*",
    "@concave/ui": "*",
    "@graphql-codegen/typescript-react-query": "^3.5.7",
    "aes256": "^1.1.0",
    "axios": "^0.26.1",
    "ethers": "^5.6.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "iron-session": "^6.0.5",
    "lightweight-charts": "^3.8.0",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-number-format": "^4.9.1",
    "react-query": "^3.34.16",
    "siwe": "^1.1.2",
    "wagmi": "^0.2.18"
  },
  "devDependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40"
  }
}
